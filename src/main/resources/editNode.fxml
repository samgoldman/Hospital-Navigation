<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXListView?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.materialicons.MaterialIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="580.0" prefWidth="949.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EditNodeController">
   <children>
      <VBox prefHeight="335.0" prefWidth="148.0" spacing="5.0">
         <children>
            <Label text="Building" />
            <JFXTextField fx:id="building_field" promptText="building" />
            <Label text="node type" />
            <JFXTextField fx:id="type_field" promptText="type" />
            <Label text="floor" />
            <JFXComboBox fx:id="floor_combo" maxWidth="1.7976931348623157E308" onAction="#comboAction" promptText="floor" />
            <Label text="Short Name" />
            <JFXTextField fx:id="short_field" promptText="short name" />
            <Label text="Long Name" />
            <JFXTextField fx:id="long_field" promptText="long name" />
            <VBox fx:id="edges_container" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox prefHeight="30.0" prefWidth="258.0">
                     <children>
                        <Label text="Edges" />
                        <Region HBox.hgrow="ALWAYS" />
                        <JFXButton fx:id="edit_show_btn" contentDisplay="GRAPHIC_ONLY" onAction="#editToggle" prefHeight="30.0" prefWidth="34.0">
                           <graphic>
                              <MaterialIconView fx:id="edit_icon_down" glyphName="KEYBOARD_ARROW_DOWN" size="20" />
                              <MaterialIconView fx:id="edit_icon_up" glyphName="KEYBOARD_ARROW_UP" size="20" />
                           </graphic>
                        </JFXButton>
                     </children>
                  </HBox>
                  <JFXListView fx:id="edges_list" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <Region VBox.vgrow="ALWAYS" />
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <JFXButton contentDisplay="GRAPHIC_ONLY" onAction="#cancelAction" prefHeight="80.0" prefWidth="80.0" text="Fulfill Request">
                     <graphic>
                        <MaterialIconView glyphName="ARROW_BACK" size="60" />
                     </graphic>
                     <HBox.margin>
                        <Insets right="8.0" />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton contentDisplay="GRAPHIC_ONLY" onAction="#saveAction" prefHeight="80.0" prefWidth="80.0" text="Fulfill Request">
                     <graphic>
                        <MaterialIconView glyphName="SAVE" size="60" />
                     </graphic>
                     <HBox.margin>
                        <Insets right="8.0" />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton contentDisplay="GRAPHIC_ONLY" onAction="#deleteAction" prefHeight="80.0" prefWidth="80.0" text="Fulfill Request">
                     <graphic>
                        <MaterialIconView glyphName="DELETE" size="60" />
                     </graphic>
                     <HBox.margin>
                        <Insets right="8.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <StackPane prefHeight="335.0" prefWidth="579.0" HBox.hgrow="ALWAYS">
         <children>
            <ScrollPane fx:id="map_scrollpane" prefHeight="200.0" prefWidth="200.0">
               <content>
                  <Pane fx:id="image_pane" onMouseClicked="#mapClickedHandler">
                     <children>
                        <ImageView>
                           <image>
                              <Image url="@Images/L1.png" />
                           </image>
                        </ImageView>
                     </children>
                  </Pane>
               </content>
            </ScrollPane>
            <Pane maxHeight="-Infinity" maxWidth="-Infinity" nodeOrientation="RIGHT_TO_LEFT" prefHeight="200.0" prefWidth="40.0" stylesheets="@theme.css" StackPane.alignment="TOP_RIGHT">
               <children>
                  <VBox fx:id="controlls_vbox" prefHeight="309.0" prefWidth="92.0">
                     <children>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Slider fx:id="zoom_slider" orientation="VERTICAL" prefHeight="127.0" prefWidth="16.0" />
                           </children>
                           <padding>
                              <Insets right="50.0" />
                           </padding>
                        </Pane>
                        <VBox fx:id="floor_change_vbox">
                           <children>
                              <JFXButton maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#floorChangeAction" prefHeight="53.0" prefWidth="98.0" text="Floor 3" />
                              <JFXButton maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#floorChangeAction" text="Floor 2" />
                              <JFXButton maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#floorChangeAction" text="Floor 1" />
                              <JFXButton maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#floorChangeAction" text="L1" />
                              <JFXButton maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#floorChangeAction" text="L2" />
                              <JFXButton maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#floorChangeAction" text="Ground" />
                           </children>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </VBox>
               </children>
               <StackPane.margin>
                  <Insets right="25.0" top="10.0" />
               </StackPane.margin>
            </Pane>
         </children>
      </StackPane>
   </children>
</HBox>
