<?import net.kurobako.gesturefx.GesturePane?>

<StackPane prefHeight="952.0" prefWidth="1920.0" stylesheets="@/theme.css" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="home.MapViewController">
    <children>
        <GesturePane fx:id="gPane" />
      <Pane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="350.0" prefWidth="137.0" stylesheets="@/theme.css" StackPane.alignment="TOP_RIGHT">
         <children>
            <HBox alignment="BOTTOM_LEFT" layoutY="-2.0" prefHeight="420.0" prefWidth="98.0" spacing="24.0">
               <children>
                  <JFXSlider fx:id="zoom_slider" orientation="VERTICAL" />
                   <VBox fx:id="floor_change_vbox" alignment="TOP_CENTER" spacing="12.0">
                       <children>
                           <JFXButton mnemonicParsing="false" onAction="#floorChangeAction" prefHeight="60.0" prefWidth="60.0" styleClass="floor-map-button" text="3" />
                           <JFXButton mnemonicParsing="false" onAction="#floorChangeAction" prefHeight="60.0" prefWidth="60.0" styleClass="floor-map-button" text="2" />
                           <JFXButton mnemonicParsing="false" onAction="#floorChangeAction" prefHeight="60.0" prefWidth="60.0" styleClass="floor-map-button" text="1" />
                           <JFXButton mnemonicParsing="false" onAction="#floorChangeAction" prefHeight="60.0" prefWidth="60.0" styleClass="floor-map-button" text="G" />
                           <JFXButton mnemonicParsing="false" onAction="#floorChangeAction" prefHeight="60.0" prefWidth="60.0" styleClass="floor-map-button" text="L1" />
                           <JFXButton mnemonicParsing="false" onAction="#floorChangeAction" prefHeight="60.0" prefWidth="60.0" styleClass="floor-map-button" text="L2" />

                       </children>
                   </VBox>
               </children>
            </HBox>
         </children>
         <StackPane.margin>
            <Insets right="24.0" top="24.0" />
         </StackPane.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Pane>
        <Pane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="347.0" prefWidth="263.0" StackPane.alignment="BOTTOM_RIGHT">
           <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="250.0" style="-fx-background-color: white;">
               <children>
                     <Label text="Call Elevator" />
                     <JFXButton fx:id="call_el1_btn" onAction="#callElevatorAction" styleClass="confirm-button" stylesheets="@/theme.css" text="Level 1">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                     </JFXButton>
                     <JFXButton fx:id="call_el2_btn" onAction="#callElevatorAction" styleClass="confirm-button" text="Level 2">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                     </JFXButton>
                     <JFXButton fx:id="call_el3_btn" onAction="#callElevatorAction" styleClass="confirm-button" text="Level 3">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                     </JFXButton>
                     <JFXButton fx:id="call_el4_btn" onAction="#callElevatorAction" styleClass="confirm-button" text="Level 4">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                     </JFXButton>
                     <Label text="Current Level" />
                     <Label fx:id="cur_el_floor" text="2">
                         <font>
                             <Font size="24.0" />
                         </font>
                     <VBox.margin>
                        <Insets bottom="6.0" />
                     </VBox.margin>
                     </Label>
               </children>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </VBox>
           </children>
        </Pane>
        </children>
</StackPane>
