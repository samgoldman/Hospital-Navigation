                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <Label maxWidth="200.0" text="Short Name" />
                  <JFXTextField fx:id="short_field" maxWidth="200.0">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <Label maxWidth="200.0" text="Long Name" />
                  <JFXTextField fx:id="long_field" maxWidth="200.0" />
               </children>
            </VBox>
            <Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
      <VBox fx:id="narnar_vbox" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="60.0">
