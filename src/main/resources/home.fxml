<VBox prefHeight="862.0" prefWidth="1395.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomeController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="101.0" prefWidth="1385.0" spacing="5.0">
         <children>
            <JFXTextField fx:id="search_bar" onAction="#searchBarEnter" prefHeight="27.0" prefWidth="217.0" text="Search room" />
            <Region HBox.hgrow="ALWAYS" />
            <Label text="Zoom" />
            <JFXButton onAction="#zoomOut" text="+" />
            <JFXSlider fx:id="zoom_slider" />
            <JFXButton onAction="#zoomIn" text="-">
               <HBox.margin>
                  <Insets right="40.0" />
               </HBox.margin></JFXButton>
            <Label text="Visuals" />
            <JFXToggleButton text="Hi Contrast" />
            <JFXToggleButton text="Large UI" />
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
      <SplitPane dividerPositions="0.2140468227424749" VBox.vgrow="ALWAYS">
         <items>
            <JFXListView />
            <ScrollPane fx:id="map_scrollpane" prefHeight="200.0" prefWidth="200.0">
               <content>
                  <Pane>
                     <children>
                        <ImageView>
                           <image>
                              <Image url="Images/1_NoIcons.png" />
                           </image>
                        </ImageView>
                     </children>
                  </Pane>
               </content>
            </ScrollPane>
         </items>
      </SplitPane>
   </children>
</VBox>
