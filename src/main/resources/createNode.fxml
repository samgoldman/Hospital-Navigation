            <VBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label maxWidth="200.0" text="Floor Level" />
                  <JFXTextField fx:id="floor_field" maxWidth="200.0">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <Label maxWidth="200.0" text="Building" />
                  <JFXTextField fx:id="building_field" maxWidth="200.0">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <Label maxWidth="200.0" text="Node Type" />
